{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "http://localhost:8765",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "webrape-dapp",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "clipboard": {
        "all": true
      },
      "dialog": {
        "all": true
      },
      "window": {
        "all": true
      },
      "shell": {
        "all": true
      },
      "protocol": {
				"all": true,
				"asset": true,
				"assetScope": ["**", "**/*"]
			}
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost; media-src 'self' asset: https://asset.localhost",
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "fund.eastmoney.com",
          "windows": [
            "main",
            "Scraper_0",
            "Scraper_1",
            "Scraper_2",
            "Scraper_3",
            "Scraper_4",
            "Scraper_5",
            "Scraper_6",
            "Scraper_7",
            "Scraper_8",
            "Scraper_9"
          ],
          "enableTauriAPI": true
        },
        {
          "domain": "quote.eastmoney.com",
          "windows": [
            "main",
            "Scraper_0",
            "Scraper_1",
            "Scraper_2",
            "Scraper_3",
            "Scraper_4",
            "Scraper_5",
            "Scraper_6",
            "Scraper_7",
            "Scraper_8",
            "Scraper_9"
          ],
          "enableTauriAPI": true
        },
        {
          "domain": "fundf10.eastmoney.com",
          "windows": [
            "main",
            "Scraper_0",
            "Scraper_1",
            "Scraper_2",
            "Scraper_3",
            "Scraper_4",
            "Scraper_5",
            "Scraper_6",
            "Scraper_7",
            "Scraper_8",
            "Scraper_9"
          ],
          "enableTauriAPI": true
        },
        {
          "domain": "finance.yahoo.com",
          "windows": [
            "main",
            "Scraper_0",
            "Scraper_1",
            "Scraper_2",
            "Scraper_3",
            "Scraper_4",
            "Scraper_5",
            "Scraper_6",
            "Scraper_7",
            "Scraper_8",
            "Scraper_9"
          ],
          "enableTauriAPI": true
        },
        {
          "domain": "s.yimg.com",
          "windows": [
            "main",
            "Scraper_0",
            "Scraper_1",
            "Scraper_2",
            "Scraper_3",
            "Scraper_4",
            "Scraper_5",
            "Scraper_6",
            "Scraper_7",
            "Scraper_8",
            "Scraper_9"
          ],
          "enableTauriAPI": true
        }
      ]
    },
    "windows": [
      {
        "label": "main",
        "title": "Webraper",
        "width": 1024,
        "height": -1,
        "resizable": true,
        "fullscreen": false,
        "x": 0,
        "y": 0
      }
    ]
  }
}