{
  "package": {
    "productName": "fund-eastmoney-test-app",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "../dist",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.fund-eastmoney.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "Fund-Eastmoney Tauri",
        "width": 800,
        "height": -1,
        "resizable": true,
        "fullscreen": false,
        "x": 0,
        "y": 0
      }
    ],
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "fund.eastmoney.com",
          "windows": ["fund_eastmoney"],
          "enableTauriAPI": true
        },
         {
          "domain": "finance.yahoo.com",
          "windows": ["finance_yahoo"],
          "enableTauriAPI": true
        },
        {
         "domain": "s.yimg.com",
         "windows": ["finance_yahoo"],
         "enableTauriAPI": true
       }
      ]
    }
  }
}